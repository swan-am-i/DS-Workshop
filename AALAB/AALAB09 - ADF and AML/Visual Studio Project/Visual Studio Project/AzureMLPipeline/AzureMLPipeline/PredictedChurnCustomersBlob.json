{
    "$schema": "http://datafactories.schema.management.azure.com/schemas/2015-08-01/Microsoft.DataFactory.Table.json",
    "name": "PredictedChurnCustomersBlob",
    "properties": {
    "structure": [
      {
        "name": "Age",
        "type": "Int16"
      },
      {
        "name": "AnnualIncome",
        "type": "Int16"
      },
      {
        "name": "CallDropRate",
        "type": "String"
      },
      {
        "name": "CallFailureRate",
        "type": "String"
      },
      {
        "name": "CustomerSuspended",
        "type": "String"
      },
      {
        "name": "Education",
        "type": "String"
      },
      {
        "name": "Gender",
        "type": "String"
      },
      {
        "name": "HomeOwner",
        "type": "String"
      },
      {
        "name": "MaritalStatus",
        "type": "String"
      },
      {
        "name": "MonthlyBilledAmount",
        "type": "Int16"
      },
      {
        "name": "NoAdditionalLines",
        "type": "String"
      },
      {
        "name": "NumberOfComplaints",
        "type": "Int16"
      },
      {
        "name": "NumberOfMonthUnpaid",
        "type": "Int16"
      },
      {
        "name": "NumDaysContractEquipmentPlanExpiring",
        "type": "Int16"
      },
      {
        "name": "Occupation",
        "type": "String"
      },
      {
        "name": "PenaltyToSwitch",
        "type": "String"
      },
      {
        "name": "State",
        "type": "String"
      },
      {
        "name": "TotalMinsUsedInLastMonth",
        "type": "Int16"
      },
      {
        "name": "UnpaidBalance",
        "type": "Int16"
      },
      {
        "name": "UsesInternetService",
        "type": "String"
      },
      {
        "name": "UsesVoiceService",
        "type": "String"
      },
      {
        "name": "PercentageCallOutsideNetwork",
        "type": "String"
      },
      {
        "name": "Totalcallduration",
        "type": "Int16"
      },
      {
        "name": "AvgCallduration",
        "type": "Int16"
      },
      {
        "name": "ScoredLabel",
        "type": "String"
      },
      {
        "name": "ScoredProbability",
        "type": "Decimal"
      }
    ],
    "type": "AzureBlob",
    "linkedServiceName": "AzureStorageLinkedService",
    "typeProperties": {
      "folderPath": "churndata/mloutput/scored/{folderpart}/",
      "fileName": "{filepart}result.csv",
      "partitionedBy": [
        {
          "name": "folderpart",
          "value": {
            "type": "DateTime",
            "date": "SliceStart",
            "format": "yyyyMM"
          }
        },
        {
          "name": "filepart",
          "value": {
            "type": "DateTime",
            "date": "SliceStart",
            "format": "HHmmss"
          }
        }
      ],
      "format": {
        "type": "TextFormat",
        "columnDelimiter": ","
      }
    },
    "availability": {
      "frequency": "Month",
      "interval": 1
    }
  }
}